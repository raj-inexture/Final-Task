<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<display-name>RegistrationServlet</display-name>
		<description></description>
		<servlet-class>controller.RegistrationServlet</servlet-class>
		<multipart-config>
			<max-file-size>20848820</max-file-size>
			<max-request-size>418018841</max-request-size>
			<file-size-threshold>1048576</file-size-threshold>
		</multipart-config>
	</servlet>
	<servlet>
		<servlet-name>ForgotPasswordUsernameServlet</servlet-name>
		<display-name>ForgotPasswordUsernameServlet</display-name>
		<description></description>
		<servlet-class>controller.ForgotPasswordUsernameServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ForgotPasswordQuestionServlet</servlet-name>
		<display-name>ForgotPasswordQuestionServlet</display-name>
		<description></description>
		<servlet-class>controller.ForgotPasswordQuestionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChangePasswordServlet</servlet-name>
		<display-name>ChangePasswordServlet</display-name>
		<description></description>
		<servlet-class>controller.ChangePasswordServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewUsersServlet</servlet-name>
		<display-name>ViewUsersServlet</display-name>
		<description></description>
		<servlet-class>controller.ViewUsersServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<display-name>LogoutServlet</display-name>
		<description></description>
		<servlet-class>controller.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserLogsServlet</servlet-name>
		<display-name>UserLogsServlet</display-name>
		<description></description>
		<servlet-class>controller.UserLogsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewAllUsersServlet</servlet-name>
		<display-name>ViewAllUsersServlet</display-name>
		<description></description>
		<servlet-class>controller.ViewAllUsersServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ForgotPasswordUsernameServlet</servlet-name>
		<url-pattern>/ForgotPasswordUsernameServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ForgotPasswordQuestionServlet</servlet-name>
		<url-pattern>/ForgotPasswordQuestionServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChangePasswordServlet</servlet-name>
		<url-pattern>/ChangePasswordServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewUsersServlet</servlet-name>
		<url-pattern>/ViewUsersServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserLogsServlet</servlet-name>
		<url-pattern>/UserLogsServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewAllUsersServlet</servlet-name>
		<url-pattern>/ViewAllUsersServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
